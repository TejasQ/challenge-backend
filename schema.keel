model Challenge {
    fields {
        isCompleted Boolean @default(false)
        name Text
        startDate Date
        endDate Date?
        penalty Number
    }

    actions {
        create createChallenge() with (name, startDate, penalty)
        list listChallenges()
        get getChallengeById(id)
        update updateChallenge(id) with (name, startDate, penalty)
        delete deleteChallenge(id)
    }
}
